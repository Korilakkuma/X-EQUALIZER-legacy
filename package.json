{
  "name": "x-equalizer",
  "version": "0.0.0",
  "engines": {
    "node": ">=16.0.0 <=18.16.x"
  },
  "description": "Chrome extension and Firefox addon for Graphic Equalizer",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint 'src/**/*.ts' && stylelint 'src/**/*.css'",
    "fix": "eslint --fix 'src/**/*.ts' && stylelint --fix 'src/**/*.css'",
    "type": "tsc --noEmit",
    "build": "npm run clean && NODE_ENV=production webpack --progress --mode production",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Korilakkuma/X-EQUALIZER.git"
  },
  "keywords": [
    "Chrome extension",
    "Firefox addon",
    "Graphic Equalizer",
    "Web Audio API",
    "XSound"
  ],
  "author": "Korilakkuma <rilakkuma.san.xjapan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Korilakkuma/X-EQUALIZER/issues"
  },
  "homepage": "https://github.com/Korilakkuma/X-EQUALIZER",
  "dependencies": {
    "xsound": "^3.1.1"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.235",
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "eslint": "^8.39.0",
    "eslint-cli": "^1.1.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "mini-css-extract-plugin": "^2.7.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "stylelint": "^15.6.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-standard": "^33.0.0",
    "terser-webpack-plugin": "^5.3.7",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  }
}
