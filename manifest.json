{
  "name": "X EQUALIZER",
  "version": "0.0.0",
  "manifest_version": 2,
  "description": "Chrome extension and Firefox addon for Graphic Equalizer",
  "icons" : {
    "128": "images/icon-128.png",
    "256": "images/icon-256.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "X EQUALIZER"
  },
  "background": {
    "scripts": [],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "css": [],
      "js": ["./dist/chrome.js"]
    }
  ],
  "permissions": [
    "tabs", "https://*/*"
  ]
}
